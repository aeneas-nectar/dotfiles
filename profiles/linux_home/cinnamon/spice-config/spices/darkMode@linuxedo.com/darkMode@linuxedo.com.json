{
    "layout": {
        "type": "layout",
        "pages": [
            "page_general",
            "page_light_mode",
            "page_dark_mode"
        ],
        "page_general": {
            "type": "page",
            "title": "General",
            "sections": [
                "section_general"
            ]
        },
        "page_light_mode": {
            "type": "page",
            "title": "Light Mode",
            "sections": [
                "section_light_mode"
            ]
        },
        "page_dark_mode": {
            "type": "page",
            "title": "Dark Mode",
            "sections": [
                "section_dark_mode"
            ]
        },
        "section_general": {
            "type": "section",
            "title": "",
            "keys": [
                "enable_dark_mode",
                "enable_auto_mode_switch",
                "light_mode_start",
                "dark_mode_start"
            ]
        },
        "section_light_mode": {
            "type": "section",
            "title": "",
            "keys": [
                "light_win_border_theme",
                "light_gtk_theme",
                "light_cinnamon_theme",
                "light_icon_theme",
                "enable_light_background_switch",
                "light_background_dir"
            ]
        },
        "section_dark_mode": {
            "type": "section",
            "title": "",
            "keys": [
                "dark_win_border_theme",
                "dark_gtk_theme",
                "dark_cinnamon_theme",
                "dark_icon_theme",
                "enable_dark_background_switch",
                "dark_background_dir"
            ]
        }
    },
    "enable_dark_mode": {
        "type": "switch",
        "description": "Dark mode",
        "default": false,
        "value": true
    },
    "enable_auto_mode_switch": {
        "type": "switch",
        "description": "Automatically switch modes",
        "default": false,
        "value": true
    },
    "light_mode_start": {
        "type": "timechooser",
        "dependency": "enable_auto_mode_switch=true",
        "description": "Light mode start time",
        "default": {
            "h": 6,
            "m": 0,
            "s": 0
        },
        "value": {
            "h": 6,
            "m": 0,
            "s": 0
        }
    },
    "dark_mode_start": {
        "type": "timechooser",
        "dependency": "enable_auto_mode_switch=true",
        "description": "Dark mode start time",
        "default": {
            "h": 18,
            "m": 0,
            "s": 0
